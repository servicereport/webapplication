@model IEnumerable<serviceReport.Models.ISO.Grupo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Anexo.Titulo)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.DominioGrupo.Titulo)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.NombreGrupo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Consecutivo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Anexo.Titulo)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.DominioGrupo.NombreDominio)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.NombreGrupo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Consecutivo)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
