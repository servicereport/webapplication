@model IEnumerable<serviceReport.Models.Auditoria.Empresa>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreEntidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreContactoEntidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailEmpresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UrlLogo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreEntidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreContactoEntidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailEmpresa)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.UrlLogo)*@
            <img width="80px" height="80px" src="@Html.DisplayFor(modelItem => item.UrlLogo)" />
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
